{
    "bodyTag": "<body>",
    "mainInput": "./src/Main.mjs",
    "mainOutput": "main.js",
    "workers": {
        "data": {
            "input": "./src/worker/Data.mjs",
            "output": "dataworker.js"
        },
        "vdom": {
            "input": "./src/worker/VDom.mjs",
            "output": "vdomworker.js"
        }
    },
    "apps": {
        "Docs": {
            "input": "./docs/app.mjs",
            "mainThreadAddons": "'DragDrop', 'HighlightJS', 'Stylesheet'",
            "output": "/docs/",
            "title": "Neo Docs"
        },
        "CovidHelix": {
            "input": "./apps/covidhelix/app.mjs",
            "output": "/apps/covidhelix/",
            "themes": "'neo-theme-dark'",
            "title": "CovidHelix"
        }
    }
}
